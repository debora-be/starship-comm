<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Stock:</strong>
    <%= product.stock %>
  </p>

  <p>
    <strong>Demand:</strong>
    <%= product.demand %>
  </p>

  <p>
    <strong>Competitor price:</strong>
    <%= product.competitor_price %>
  </p>

  <p>
    <strong>Created at:</strong>
    <%= product.created_at %>
  </p>

  <p>
    <strong>Updated at:</strong>
    <%= product.updated_at %>
  </p>

</div>
